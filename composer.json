{
    "name": "giosh94mhz/geonames-bundle",
    "description": "Symfony2 bundle to import and use Geonames toponyms.",
    "keywords": ["symfony", "bundle", "geonames"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Giorgio Premi",
            "email": "giosh94mhz@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/framework-bundle": "~2.3",
	"symfony/console": "~2.3",
        "guzzle/guzzle" : "~3"
    },
    "require-dev": {
        "doctrine/orm" : "~2.4",
        "willdurand/geocoder" : "~2",
        "willdurand/geocoder-bundle" : "~3"
    },
    "autoload": {
        "psr-0": { "Giosh94mhz\\GeonamesBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Giosh94mhz/GeonamesBundle"
}
